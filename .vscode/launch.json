{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "lldb",
			"request": "launch",
			"name": "(lldb) Minishell",
			"program": "${workspaceFolder}/minishell",
			"args": [],
			"cwd": "${workspaceFolder}",
			"preLaunchTask": "make"
		},
		{
			"name": "(gdb) minishell",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/minishell",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"preLaunchTask": "make",
			"setupCommands": [
			  {
				"description": "Enable pretty-printing for gdb",
				"text": "-enable-pretty-printing",
				"ignoreFailures": true
			  },
			  {
				"description": "Disable breaking on SIGINT",
				"text": "handle SIGINT nostop noprint pass"
			},
			{
				"description": "Disable breaking on SIGQUIT",
				"text": "handle SIGQUIT nostop noprint pass"
			},
			]
		},
	]
}
